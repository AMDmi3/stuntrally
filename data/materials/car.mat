material car_base
{
	parent base
	terrain_light_map true
	terrain_light_map_toggleable true
}

material car_body
{
	parent car_base
	diffuseMap body00_add.png
	carPaintMap body00_red.png
	reflMap refl.png
	env_map true

	fresnel true
	fresnelScaleBiasPower 0.81 0.10 3
	specular 1 1.05 1.1  20 //x4
	glossiness 0.5
	fresnelScaleBiasPower2 0.71 0.10 6
	specular2 0.7 0.65 0.6  1 //x4
}

material car_glass
{
	parent car_base
	diffuseMap glass.png
	depth_write off
	scene_blend alpha_blend
	ambient 1.9 1.9 1.9
	diffuse 1.9 1.9 1.9
	specular 1.5 1.5 1.5  96 //x4

	env_map true
	fresnel true
	specular_alpha true
	//cull_hardware none
	twoside_diffuse true

	fresnelScaleBiasPower 0.80 0.3 4
	//  env clr mul,  al clr mul,  al clr pow,  al ifluence mul fin
	//env_alpha 1.4  1.6 2  0.6
	//env_alpha 1.5  1.1 1.5  0.8
	env_alpha 1.1  1.1 1.1  0.8
}

material car_ghost
{
	parent base
	diffuseMap white.png
}

material car_interior
{
	parent car_base
	diffuseMap interior.png
}

material car_wheel
{
	parent car_base
