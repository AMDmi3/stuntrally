material road_terrain
{
	parent base
    depth_write off
    scene_blend alpha_blend
    alphaMap roadAlpha.png
    //alphaMap noise.jpg
    depth_bias 3
    terrain_light_map true
	road_blend true
}

//  bridges
material road
{
    parent base
    scene_blend alpha_blend
    alphaMap white.png
    depth_bias 3
	road_blend true
}


