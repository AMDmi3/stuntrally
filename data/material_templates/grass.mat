material grass
{
    parent base
    alpha_rejection greater_equal 128
    cull_hardware none
    scene_blend alpha_blend
    grass_wind true
    vertex_colour true
    terrain_light_map true
    mrt_output false
    diffuse 0.8 0.8 0.8
}

material grassAustralia
{
    parent grass
    diffuseMap grassAustralia.png
}

material grassDesert
{
    parent grass
    diffuseMap grassDesert.png
}

material grassForest
{
    parent grass
    diffuseMap grassForest.png
}

