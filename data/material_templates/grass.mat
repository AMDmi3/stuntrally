material grass
{
    parent base
    alpha_rejection greater_equal 128
    cull_hardware none
    scene_blend alpha_blend
    grass_wind true
    vertex_colour true
    terrain_light_map true
    mrt_output false
	ambient 2.0 2.0 2.0
	diffuse 0.1 0.1 0.1
	specular 0.1 0.1 0.1 16
}

material grassAustralia
{
    parent grass
    diffuseMap grassAustralia.png
}

material grassDesert
{
    parent grass
    diffuseMap grassDesert.png
}

