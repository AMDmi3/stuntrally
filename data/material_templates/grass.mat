material grass
{
    parent base
    alpha_rejection greater_equal 128
    cull_hardware none
    scene_blend alpha_blend
    grass_wind true
    vertex_colour true
    terrain_light_map true
    mrt_output false
    diffuse 0.8 0.8 0.8
    diffuse_address_mode clamp
}

material grassAustralia
{
    parent grass
    diffuseMap grassAustralia.png
}

material grassDesert
{
    parent grass
    diffuseMap grassDesert.png
}

material grassForest
{
    parent grass
    diffuseMap grassForest.png
}

material grassJungle
{
    parent grass
    diffuseMap grassJungle.png
    ambient 1.4 1.5 1.4
    diffuse 0.5 0.5 0.5
}

material grassGreece
{
    parent grass
    diffuseMap grassGreece.png
}

material grassSavanna
{
    parent grass
    diffuseMap grassSavanna.png
}

material grassWinter
{
    parent grass
    diffuseMap grassWinter.png
}

material grassUnidentified
{
    parent grass
    diffuseMap grassUnidentified.png
    diffuse 0.4 0.4 0.4
}

