material Particles/Spark
{
	technique
	{
		pass
		{
			lighting off
			depth_write off
			scene_blend add

			texture_unit
			{
				texture spark.png
			}
		}
	}
}

material Boost
{
	receive_shadows off
	technique
	{
		pass
		{
			lighting off
			scene_blend add
			depth_write off
			diffuse vertexcolour
			//transparent_sorting off

			texture_unit
			{
				tex_address_mode clamp
				texture boost.png
			}
		}
	}
}
